Account (firstname, lastname, accnumber)
List<Account>   10 object




Class Account implements Comparator<Account>{
	String firstname, String  lastname, int accnumber;
	
	@Override
	public int compare(Accont obj1, Accont obj2 ) {
		obj1.firstname.compareTo(obj2.firstname);
	}
}

